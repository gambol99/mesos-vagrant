{
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "registry",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 5000, "hostPort": 5000, "protocol": "tcp"}
      ]
    }
  },
  "id": "docker-registry",
  "instances": "1",
  "constraints": [["hostname", "UNIQUE"]],
  "cpus": "0.03",
  "mem": "128"
}

