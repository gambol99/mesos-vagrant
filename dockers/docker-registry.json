{
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "registry",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 5000, "hostPort": 5000, "protocol": "tcp"}
      ]
    }
  },
  "env": {
    "APP": "docker-register",
    "ENVIRONMENT": "prod",
    "NAME": "register"
  },
  "id": "docker-registry",
  "instances": "1",
  "cpus": "0.1",
  "mem": "64"
}

