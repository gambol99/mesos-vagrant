{
  "id": "/service/registrar",
  "instances": "1",
  "cpus": "0.1",
  "mem": "64",
  "uris": [],
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "hub101:5000/service-registrar",
      "network": "BRIDGE",
      "portMappings": [{
         "containerPort": 22,
         "hostPort": 0,
         "protocol": "tcp",
         "servicePort": "22"
        }
      ]
    },
  },
  "env": {
    "APP": "registrar",
    "NAME": "registrar",
    "ENVIRONMENT": "prod",
    "ETCD_HOST": "10.0.1.100"
  },
  "volumes": [{
    "containerPath": "/var/run/docker.sock",
    "hostPath": "/var/run/docker.sock",
    "mode": "RW"
  }]
}

