{
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "ubuntu",
      "network": "CONTAINER:proxy",
      "portMappings": [
        { 
           "containerPort": 8080,
           "hostPort": 0,
           "protocol": "tcp",
           "servicePort": "8080"
        }
      ]
    }
  },
  "id": "/frontend/web",
  "instances": "1",
  "cpus": "0.2",
  "mem": "64",
  "uris": [],
  "cmd": "while sleep 10; do date -u +%T; done"
}

