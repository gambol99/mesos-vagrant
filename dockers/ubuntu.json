{
  "id": "ubuntu",
  "cpus": 0.1,
  "mem": 128.0,
  "instances": 1,
  "args": ["/bin/sh", "-c", "env && sleep 300"],
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "ubuntu",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 6379, "hostPort": 0, "protocol": "tcp"}
      ]
    }
  },
  "env": {
    "APP": "ubuntu",
    "NAME": "ubuntu",
    "HOSTNAME": "$HOSTNAME",
    "ENVIRONMENT": "prod"
  }
}
