{
  "id": "redis",
  "cpus": 0.1,
  "mem": 128.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "redis",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 6379, "hostPort": 0, "protocol": "tcp"}
      ]
    }
  },
  "env": {
    "APP": "redis",
    "NAME": "redis-slaves",
    "ENVIRONMENT": "prod"
  }
}
