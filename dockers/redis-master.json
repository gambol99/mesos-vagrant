{
  "id": "/redis",
  "groups": [{
    "id": "/redis/master",
    "apps": [{
      "id": "/redis/master",
      "container": {
        "type": "DOCKER",
        "docker": {
          "image": "redis",
          "network": "BRIDGE",
          "portMappings": [
            {
               "containerPort": 6379,
               "hostPort": 0,
               "protocol": "tcp",
               "servicePort": "6379"
            }
          ]
        }
      },
      "instances": "1",
      "cpus": "0.1",
      "mem": "64",
      "uris": []
    }]
  }]
}
